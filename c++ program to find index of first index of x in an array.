#include <iostream>
using namespace std;

int main() {
    //Write your code here
int n;
    int t=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int x;
    cin>>x;
    for(int i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            cout<<i;
            t=1;
            break;
        }
    }if(t==0)
        cout<<-1;
    return 0;
}
